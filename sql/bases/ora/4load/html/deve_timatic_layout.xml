<?xml version="1.0" encoding="cp866"?>
<layout>
  <ctrl_list>
    <ctrl>
      <type>page</type>
      <name>pageControl</name>
      <align>alClient</align>
      <tab_list>
        <tab>
          <caption>Document</caption>
          <tab_type>Document</tab_type>
          <ctrl_list>
            <ctrl>
              <type>scrollBox</type>
              <name>DocumentSBox</name>
              <align>alClient</align>
              <ctrl_list>
                <ctrl>
                  <type>label</type>
                  <top>1</top>
                  <left>1</left>
                  <color>Navy</color>
                  <name>DestinationLbl</name>
                  <caption>Страна прибытия</caption>
                </ctrl>
                <ctrl>
                  <type>label</type>
                  <top>30</top>
                  <left>1</left>
                  <color>Navy</color>
                  <name>NationalityLbl</name>
                  <caption>Национальность</caption>
                </ctrl>
                <ctrl>
                  <type>edit</type>
                  <top>1</top>
                  <left>150</left>
                  <name>Destination</name>
                  <MaxLength>3</MaxLength>
                  <width>50</width>
                  <ref>etCountry</ref>
                  <mandatory/>
                </ctrl>
                <ctrl>
                  <type>edit</type>
                  <top>30</top>
                  <left>150</left>
                  <name>Nationality</name>
                  <MaxLength>3</MaxLength>
                  <width>50</width>
                  <ref>etCountry</ref>
                  <mandatory/>
                </ctrl>
                <ctrl>
                  <type>label</type>
                  <top>60</top>
                  <left>1</left>
                  <name>DocumentTypeLbl</name>
                  <caption>Тип документа</caption>
                </ctrl>
                <ctrl>
                  <type>cbox</type>
                  <top>60</top>
                  <left>150</left>
                  <name>DocumentType</name>
                  <width>210</width>
                  <source>documentType</source>
                </ctrl>
                <ctrl>
                  <type>label</type>
                  <top>90</top>
                  <left>1</left>
                  <name>SectionLbl</name>
                  <caption>Раздел</caption>
                </ctrl>
                <ctrl>
                  <type>cbox</type>
                  <top>90</top>
                  <left>150</left>
                  <name>Section</name>
                  <width>210</width>
                  <source>dataSection</source>
                </ctrl>
                <ctrl>
                  <type>label</type>
                  <top>120</top>
                  <left>1</left>
                  <name>DocumentGroupLbl</name>
                  <caption>Группа документа</caption>
                </ctrl>
                <ctrl>
                  <type>cbox</type>
                  <top>120</top>
                  <left>150</left>
                  <name>DocumentGroup</name>
                  <width>210</width>
                  <source>documentGroup</source>
                </ctrl>
                <ctrl>
                  <type>label</type>
                  <top>150</top>
                  <left>1</left>
                  <name>ArrivalDateLbl</name>
                  <caption>Arrival Date</caption>
                </ctrl>
                <ctrl>
                  <type>date</type>
                  <top>150</top>
                  <left>150</left>
                  <name>ArrivalDate</name>
                  <width>90</width>
                </ctrl>
                <ctrl>
                  <type>label</type>
                  <top>180</top>
                  <left>1</left>
                  <color>Navy</color>
                  <name>CarrierCodeLbl</name>
                  <caption>Carrier Code</caption>
                </ctrl>
                <ctrl>
                  <type>edit</type>
                  <top>180</top>
                  <left>150</left>
                  <name>CarrierCode</name>
                  <MaxLength>3</MaxLength>
                  <width>50</width>
                  <ref>etAirline</ref>
                </ctrl>
                <ctrl>
                  <type>label</type>
                  <top>210</top>
                  <left>1</left>
                  <name>secondaryDocLbl</name>
                  <caption>Secondary Document</caption>
                </ctrl>
                <ctrl>
                  <type>cbox</type>
                  <top>210</top>
                  <left>150</left>
                  <name>secondaryDoc</name>
                  <width>210</width>
                  <source>secondaryDocumentType</source>
                </ctrl>
              </ctrl_list>
            </ctrl>
          </ctrl_list>
        </tab>
        <tab>
          <caption>Parameter List</caption>
          <tab_type>Param</tab_type>
          <ctrl_list>
            <ctrl>
              <type>scrollBox</type>
              <name>ParamSBox</name>
              <align>alClient</align>
              <ctrl_list>
                <ctrl>
                  <type>label</type>
                  <top>1</top>
                  <left>1</left>
                  <color>Navy</color>
                  <name>ParamCountryLbl</name>
                  <caption>Country</caption>
                </ctrl>
                <ctrl>
                  <type>edit</type>
                  <top>1</top>
                  <left>150</left>
                  <name>ParamCountry</name>
                  <MaxLength>2</MaxLength>
                  <width>50</width>
                  <ref>etCountry</ref>
                  <mandatory/>
                </ctrl>
                <ctrl>
                  <type>label</type>
                  <top>30</top>
                  <left>1</left>
                  <name>ParamSectionLbl</name>
                  <caption>Section</caption>
                </ctrl>
                <ctrl>
                  <type>cbox</type>
                  <top>30</top>
                  <left>150</left>
                  <name>ParamSection</name>
                  <width>210</width>
                  <source>dataSection</source>
                  <mandatory/>
                </ctrl>
              </ctrl_list>
            </ctrl>
          </ctrl_list>
        </tab>
      </tab_list>
    </ctrl>
  </ctrl_list>
</layout>
